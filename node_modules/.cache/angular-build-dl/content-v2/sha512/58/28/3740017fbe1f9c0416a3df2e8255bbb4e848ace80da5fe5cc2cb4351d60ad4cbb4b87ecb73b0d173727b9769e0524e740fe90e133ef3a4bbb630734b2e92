outeConfig.tripDetail.url], { queryParams: {tripId: id}})\n  }\n\n  checkKeyPresent(obj){\n    if(obj){\n      let key  = Object.keys(obj)\n      if(key.length) return true;\n      else return false;\n    }\n    else{\n      return false;\n    }   \n  }\n\n\n  routeToDriverDetail(id) {\n    this.localService.setItem('driverId', id)\n    this.router.navigate([RouteConfig.driverDetail.url])\n  }\n\n}\n","<!-- <div class=\"tripSubBox\" *ngFor=\"let item of tableData; let i=index\" (click)=\"routeToTripDetailInfo(item?.id)\">\n    <div class=\"tripFirstBox\" >\n        <p class=\"detailNumber\">S No: <span>{{i+1}}</span></p>\n        <span><img src=\"assets/images/border-right.png\"></span>\n        <p class=\"detailNumber driverNameText\">{{\"driver\" | translate}}:<span>{{item?.driver_detail?.first_name || 'NA'}} {{item?.driver_detail?.last_name || ''}}</span></p>\n        <span><img src=\"assets/images/border-right.png\"></span>\n        <p class=\"detailNumber\">{{\"v.license\" | translate}}:<span>{{item?.license_plate || 'NA'}}</span></p>\n        <span><img src=\"assets/images/border-right.png\"></span>\n        <p class=\"detailNumber totalTime\">\n            {{\"t.totalTime\" | translate}}:<span>{{calcTime(item?.stop_date_time, item?.start_date_time)}}</span></p>\n        <span><img src=\"assets/images/border-right.png\"></span>\n         <p class=\"detailNumber safeScore\">{{\"t.safeScore\" | translate}}: <span>{{service.convertToDutch(item?.safe_score) || '0'}}</span></p>\n       <div class=\"warningImg\">\n        <div class=\"warningDrop \" *ngIf=\"checkKeyPresent(toolTipData[item.id])\">\n            <span class=\"cursor\" (mouseenter)=\"onMouseEnter(item,i)\" (mouseleave)=\"onMouseEnter(item,i)\"><img src=\"assets/images/warning-2.png\"></span>\n            <div class=\"waringDropDown\" *ngIf=\"showPopupIndex == i\" >\n                <div class=\"dataAcceleration\" *ngIf=\"toolTipData[item.id]['acceleration']\">\n                    <p class=\"warningTit